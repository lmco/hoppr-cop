{
  "name": "hoppr-cop",
  "version": "0.18.3",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf publifc && ts-node -T tools/build --optimize",
    "build:all": "rimraf public && ts-node -T tools/build --all --optimize",
    "build:dirty": "ts-node -T tools/build --dirty",
    "clean": "rimraf public",
    "check": "run-p check:*",
    "check:build": "tsc --noEmit",
    "check:style": "run-p check:style:*",
    "check:style:scss": "stylelint \"overrides/**/*.scss\"",
    "check:style:ts": "eslint --cache \"overrides/**/*.ts\"",
    "start": "ts-node -T tools/build --verbose --all --dirty --watch",
    "upgrade": "run-s upgrade:*",
    "upgrade:bump": "ncu --upgrade --filterVersion \"/^\\^/\"",
    "upgrade:install": "npm install",
    "copy:bundle": "cp public/overrides/assets/javascripts/bundle.*.min.js public/assets/javascripts/bundle.js",
    "copy:home": "cp public/overrides/assets/stylesheets/home.*.min.css public/assets/stylesheets/home.css",
    "copy:main": "cp public/overrides/assets/stylesheets/main.*.min.css public/assets/stylesheets/main.css",
    "prepare:overrides": "ts-node-dev -T tools/build --all --optimize --verbose",
    "prepare:webpage": "yarn prepare:overrides && yarn copy:bundle && yarn copy:home && yarn copy:main && cp public/overrides/* public/ -r",
    "make:hoppr": "poetry run mkdocs build --strict --verbose --no-directory-urls && yarn prepare:webpage",
    "make:hoppr:prod": "poetry run mkdocs build --strict --verbose --no-directory-urls && yarn prepare:webpage"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "array-flat-polyfill": "^1.0.1",
    "clipboard": "^2.0.11",
    "escape-html": "^1.0.3",
    "focus-visible": "^5.2.0",
    "fuzzaldrin-plus": "^0.6.0",
    "iframe-worker": "^1.0.0",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.9.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.5.5",
    "unfetch": "^5.0.0",
    "url-polyfill": "^1.1.12"
  },
